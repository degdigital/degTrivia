{
	"presets": [
		["env", {
			"debug": true,
			"modules": false,
      		"targets": {
        		"browsers": [">1%", "ie 11", "not op_mini all"]
     	 	}
    	}]
    ],
	"plugins": [
		"transform-runtime",
		"external-helpers",
	 	"syntax-async-functions",
	    ["transform-regenerator", {
	    	"asyncGenerators": false,
	    	"generators": false,
	    	"async": true
		}]
	],
  	"env": {
		"test": {
	    	"ignore": [
				"node_modules/**"
			],
	      	"presets": [
	      		["env", {
	      			"targets": {
	      				"node": "current"
	      			}
	      		}]
	      	]
	    }
	}
}